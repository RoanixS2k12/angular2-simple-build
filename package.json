{
  "name": "angular2-min",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "serve": "http-server . -p 5556",
    "build:dev": "npm run clean && tsc",
    "build:prod": "npm run build:dev && browserify -s main dist/main.js > dist/bundle.js && npm run minify",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js"
  },
  "author": "Minko Gechev <mgechev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "browserify": "^13.0.1",
    "http-server": "^0.9.0",
    "typescript": "^2.0.3",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "@angular/common": "^2.1.0",
    "@angular/compiler": "^2.1.0",
    "@angular/core": "^2.1.0",
    "@angular/platform-browser": "^2.1.0",
    "@angular/platform-browser-dynamic": "^2.1.0",
    "es6-shim": "^0.35.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.25"
  }
}
